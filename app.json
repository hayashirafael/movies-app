{
  "expo": {
    "name": "movies-app",
    "slug": "movies-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#16171B"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash.png",
        "backgroundColor": "#16171B"
      }
    },
    "web": {
      "icon": "./src/assets/icon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}